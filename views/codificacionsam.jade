                          td ACTIVIDAD
                          td UNIDAD
                          td DESCRIPCION
                        tbody
                          td
                            .form-group.has-error.has-feedback(style='margin-bottom: 0',id='columnaactividad'+i+'')
                              .col-sm-12
                                input.form-control.input-lg.grupo1(type='number', placeholder='Ejemplo: 2010' id="d"+i+"")
                                span(class="glyphicon glyphicon-remove form-control-feedback")
                          td
                            .form-group.has-error.has-feedback(style='margin-bottom: 0',id='columnaunidad'+i+'')
                              .col-sm-12
                                input.form-control.input-lg.grupo2(type='text', placeholder='Ejemplo: H-H' id="d"+i+"")
