extends layout2

block content
    ul.nav.nav-tabs.nav-justified(role='tablist',style="padding-bottom:0px;padding-top:80px;background:#fff")
        li.active(role='presentation') 
          a(href='#actividadessam', aria-controls='sam', role='tab', data-toggle='tab') ACTIVIDADES
        li(role='presentation')
          a(href='#codpersonal', aria-controls='personal', role='tab', data-toggle='tab') PERSONAL
        li(role='presentation')
          a(href='#codvehiculo', aria-controls='vehiculo', role='tab', data-toggle='tab') VEHICULOS
        li(role='presentation')
          a(href='#codmateriales', aria-controls='materiales', role='tab', data-toggle='tab') MATERIALES
        li(role='presentation')
          a(href='#salarios', aria-controls='salarios', role='tab', data-toggle='tab') SALARIOS

    .tab-content
      #actividadessam.tab-pane.active(role='tabpanel')
        .row(style='background:#fff;padding-top:15px;padding-bottom:15px') 
          .col-md-12
            .col-md-4.col-md-offset-4
              a(class="btn" href="#ContentAsignarUser" role="button",id='btnradioRojo',style='width:70%;text-align:center;margin: 0 auto;display:block;') NUEVA CODIFICACION
        .row(style='background:#fff;padding-bottom:0px;')
          .col-md-12
            table.table.table-responsive(style="width:95%;margin:0 auto;background:#fff")
              thead
                tr
                  td N°
                  td ACTIVIDAD
                  td UNIDAD
                  td DESCRIPCION
              tbody#tablacodactividades
                if sam[0].estado == true
                  -var aux=sam[0].codsam.length
                  if aux>10
                    - aux=10
                  - for (var x=0;x<aux;x++)
                    if x%2
                      tr.info
                        td #{x+1}
                        td #{sam[0].codsam[x]}
                        td #{sam[0].unidad[x]}
                        td #{sam[0].descripcion[x]}
                    else
                      tr
                        td #{x+1}
                        td #{sam[0].codsam[x]}
                        td #{sam[0].unidad[x]}
                        td #{sam[0].descripcion[x]}
                else
                  .row(style='background:#fff')
                    .col-md-8.col-md-offset-2
                      .alert.alert-danger.alert-dismissible(role='alert',id='alertaSamError',style='margin:20px;')
                        button.close(type='button', data-dismiss='alert', aria-label='Close')
                          span(aria-hidden='true') ×
                        strong NO SE ENCONTRARON RESULTADOS!
                        |  Aun no se registro Ningun Codigo .. Registre actividades para evitar problemas en el sistema.
        .row(style=('background:#fff'))
          .col-md-12
            #page-codactividades

        .row(style='background:rgba(58, 148, 240, 0.8);padding-bottom:50px;',id='ContentAsignarUser')
          .col-md-12
            .form-horizontal(style='margin-top:30px;',id='formAsignarUser')
              .form-group
                h1(style='color:#fff;font-weight:600;margin-bottom:20px') REGISTRO DE NUEVA ACTIVIDAD SAM
              - for(var i=0;i<13;i++)
                    if i==0
                      table.table(style='width:70%;margin:0 auto;background:#fff',id='asignaractividades')
                        thead(style='background: #000')
                          td ACTIVIDAD
                          td UNIDAD
                          td DESCRIPCION
                        tbody
                          td
                            .form-group.has-error.has-feedback(style='margin-bottom: 0',id='columnaactividad'+i+'')
                              .col-sm-12
                                input.form-control.input-lg.grupo1(type='number', placeholder='Ejemplo: 2010' id="d"+i+"")
                                span(class="glyphicon glyphicon-remove form-control-feedback")
                          td
                            .form-group.has-error.has-feedback(style='margin-bottom: 0',id='columnaunidad'+i+'')
                              .col-sm-12
                                input.form-control.input-lg.grupo2(type='text', placeholder='Ejemplo: H-H' id="d"+i+"")
                                span(class="glyphicon glyphicon-remove form-control-feedback")
                          td
                            .form-group.has-error.has-feedback(style='margin-bottom: 0',id='columnadescripciond'+i+'')
                              .col-sm-12
                                input.form-control.input-lg.grupo3(type='text', placeholder='Ejemplo: Reposicion de Ripio' id="d"+i+"")
                                span(class="glyphicon glyphicon-remove form-control-feedback")
