                td Programación Quincenal
                td Limbert Arando Benavides
                td 2016-03-12
      #combustibles.tab-pane(role='tabpanel')
        .row(style='padding-bottom:30px;padding-top:30px')
          table.table.table-responsive.table-bordered(style="width:85%;margin:0 auto")
            thead
              tr
                th N°
                th CATEGORIA
                th USUARIO
                th FECHA
            tbody#contentReports
              tr(type='button', data-toggle='modal', data-target='.bs-example-modal-lg',value="DEFINICIONESDEDISENO")
                td 1
                td Programación Quincenal
                td Limbert Arando Benavides
                td 2016-03-12   
  .modal.fade.bs-example-modal-lg(tabindex='-1', role='dialog', aria-labelledby='myLargeModalLabel')
    .modal-dialog.modal-lg(role='document',style="margin-top:5px;margin-bottom:5px")
      .modal-content
        .modal-header
          button.close(type='button', data-dismiss='modal', aria-label='Close')
            span(aria-hidden='true') ×
          h4#myModalLabel.modal-title REPORTES RECIBIDOS
        .modal-body(style='height:90vh;padding:0')
          #example1
  script(src="/javascripts/bootstrap.min.js")