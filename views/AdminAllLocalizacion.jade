extends layoutGeo

block content
    .row(style="width:100%;margin:0;")
        nav.navbar.navbar-inverse(style="margin:0;")
            .container-fluid(style="padding-left:0;")
                .collapse.navbar-collapse(id="bs-example-navbar-collapse-1",style="padding-left:0;")
                    ul.nav.navbar-nav.col-md-12(id="global-actions",style="padding-left:0;")
                        li(class="col-md-1",style="padding-left:0;padding-right:0;")
                            a.menuUs(style="padding-right:0;text-align:center;")
                                span(class="glyphicon glyphicon-align-justify", aria-hidden="true")
                        li(class="col-md-2 col-sm-2 col-xs-12",style="padding-left:0;")
                            img(src="/images/logoSEDECA.png",class="logoSEDECA")
                        li(class="col-md-1")
                            a(href="MenuPrincipal",style="text-align:center;")
                                span(class="glyphicon glyphicon-home", aria-hidden="true") 
                        li(class="col-md-2")
                            a(href='Principal_TiempoReal',style="padding-left:0;padding-right:0;text-align:center;") PRINCIPAL
                                span(class="glyphicon glyphicon-map-marker", aria-hidden="true")
                        li.active(class="col-md-2")
                            a(href="Localizacion_All_TR",style="padding-left:0;padding-right:0;text-align:center;") LOCALIZAR
                                span(class="glyphicon glyphicon-map-marker", aria-hidden="true")
                        li(class="dropdown col-md-2")
                            a(href="#", class="dropdown-toggle", data-toggle="dropdown", role="button", aria-haspopup="true", aria-expanded="false",style="padding-left:0;padding-right:0;text-align:center;") MOSTRAR MAS
                                span(class="caret")
                                    ul(class="dropdown-menu")                    
                                        li
                                            a(href="Usuarios") Usuarios
                                        li
                                            a(href="RegistrarUsuario") Registrar Usuarios
                                        li
                                            a(href='#',id='btnMensajes', data-toggle="modal" data-target="#myModal") Mensajes
                                        li
                                            a(href="AdminTRealPrincipal") Localizacion GPS
                        li(class="dropdown col-md-2 col-md-offset-3 avatarrr")
    .row
        .col-md-2(style="margin:0;padding-right:0;")
            .row.localizadosTile
                h4(style="margin:0; text-align:center;background:#898989;color:white; font-weight:bold;padding-left:5px;") USUARIOS 
                h4(style="margin:0; text-align:center;background:#898989;color:white; font-weight:bold;padding-left:5px;") LOCALIZADOS
            .row(style="margin:0;overflow-y:scroll;height:574px;background:white;")
                table.table.table-hover.ListaLocalizados
        .col-md-10.mapa(style="padding:0;")
            #mimapa(style="top: 55px;margin:0;width:84%;height: 92%;")
    script(src="/javascripts/bootstrap.min.js")